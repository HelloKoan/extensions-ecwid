{
  "version": "1.0.0-rc",
  "title": "Ecwid",
  "description": "C# services and models classes to Ecwid API v1 and v3",

  "packOptions": {
    "iconUrl": "https://dj925myfyz5v.cloudfront.net/favicon_v2.ico",
    "licenseUrl": "https://raw.githubusercontent.com/kroniak/extensions-ecwid/master/LICENSE",
    "projectUrl": "https://github.com/kroniak/extensions-ecwid",
    "releaseNotes": "",
    "repository": {
      "type": "git",
      "url": "https://github.com/kroniak/extensions-ecwid.git"
    },
    "requireLicenseAcceptance": false,
    "summary": "C# services and models classes to Ecwid API v1 and v3",
    "tags": [ "ecwid api client rest async" ]
  },

  "authors": [ "me@kroniak.net" ],
  "copyright": "Copyright ©2016 by me@kroniak.net",
  "license": "GPL-3.0",

  "buildOptions": {
    "compile": {
      "include": [ "../Ecwid.Models/**/*.cs", "../Ecwid.Services/**/*.cs" ]
    },
    "warningsAsErrors": true,
    "xmlDoc": true
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Flurl.Http": "1.0.1"
  },

  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      }
    },
    "netstandard1.4": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12"
      }
    }
  }
}